## BASE PATHS
HOME="/Users/$USER"
BREW_HOME="/usr/local"
MY_HOME="$HOME/usr/local"
MY_DOCS="$HOME/Documents"
MY_APPS="/Applications"

## JAVA PATHS
JAVA_HOME="/Library/Java/JavaVirtualMachines/jdk$v_JRE.jdk/Contents/Home"
JVM_HOME="/Library/Java/JavaVirtualMachines/jdk$v_JRE.jdk/Contents/Home"
JDK_HOME="/Library/Java/JavaVirtualMachines/jdk$v_JDK.jdk/Contents/Home"
JAVA_OPTS="-Xms512m -Xmx2048m -Djava.awt.headless=true"

## CUSTOM PATHS
ACTIVEMQ_HOME="$MY_HOME/activemq"
ANDROID_HOME="$MY_HOME/android-sdk-macosx"
ANT_HOME="$MY_HOME/ant"
AWS_CREDENTIALS_FILE="$MY_HOME/aws/aws-credentials"
CATALINA_HOME="$BREW_HOME/Cellar/tomcat/$v_TOMCAT/libexec"
CF_HOME="/Applications/ColdFusion10/cfusion"
CF_ROOT="$CF_HOME/wwwroot/WEB-INF"
COREUTILS="$BREW_HOME/opt/coreutils/libexec/gnubin"
CPATH="/Applications/Xcode.app/Contents/Developer/Platforms/MacOSX.platform/Developer/SDKs/MacOSX.sdk/usr/include"
DROPBOX_HOME="$MY_DOCS/Dropbox/code/app"
GOROOT="$BREW_HOME/Cellar/go/$v_GO/libexec"
GOPATH="$HOME/Dropbox/code/go"
GRADLE_HOME="$BREW_HOME/Cellar/gradle/$v_GRADLE"
GRAILS_HOME="$BREW_HOME/opt/grails/libexec"
GROOVY_HOME="$BREW_HOME/opt/groovy/libexec"
HEROKU_HOME="$BREW_HOME/heroku/"
JETTY_HOME="$BREW_HOME/Cellar/jetty/$v_JETTY/libexec"
JETTY_BASE="$HOME/Sites/www/app/jetty"
JENKINS_HOME="$HOME/Dropbox/code/app/jenkins"
JRUBY_HOME="$BREW_HOME/Cellar/jruby/$v_JRUBY"
JYTHON_HOME="$BREW_HOME/Cellar/jython/$v_JYTHON/libexec"
LIQUIBASE_HOME=$BREW_HOME/Cellar/liquibase/$v_LIQUIBASE/libexec
M2_HOME="$MY_HOME/maven"
MAVEN_OPTS="-Xms256m -Xmx512m"
MYSQL_PATH="$BREW_HOME/Cellar/mariadb/$v_MARIADB"
MONGODB_HOME="$BREW_HOME/Cellar/mongodb/$v_MONGODB"
NGINX_HOME="$BREW_HOME/Cellar/nginx/$v_NGINX"
PYTHON_HOME="/System/Library/Frameworks/Python.framework/Versions/Current"
RUBY_HOME="$HOME/.rvm/rubies/ruby-$v_RUBY/bin/ruby"
WEBMIN_HOME="$MY_HOME/webmin"
VISUALVM="$MY_HOME/visualvm"
LDFLAGS="-L/usr/local/opt/zlib/lib:-L/usr/local/opt/libffi/lib"
CPPFLAGS="-I/usr/local/opt/zlib/include:-I/usr/local/opt/libffi/include"
BOGUS="A whole lot of who?"

CLASSPATH="$JYTHON_HOME/jython.jar:\
$CF_HOME/lib/customlib/h2.jar:\
$CF_HOME/lib/customlib/hsqldb.jar:\
$CF_HOME/lib/customlib/jtds-1.2.5.jar:\
$CF_HOME/lib/customlib/mongo-2.7.0.jar:\
$CF_HOME/lib/customlib/sqlitejdbc-v037-nested.jar:\
$CLASSPATH"
JYTHONPATH="$JYTHON_HOME:$JYTHON_HOME/bin:$JYTHON_HOME/Lib"
PYTHONPATH="$PYTHON_HOME/bin"
PERL5LIB="~/perl5/lib/perl5${PERL5LIB:+:${PERL5LIB}}"
PERL_LOCAL_LIB_ROOT="~/perl5${PERL_LOCAL_LIB_ROOT:+:${PERL_LOCAL_LIB_ROOT}}"
PERL_MB_OPT="--install_base \"~/perl5\""
PERL_MM_OPT="INSTALL_BASE=~/perl5"
PKG_CONFIG_PATH="/usr/local/opt/libffi/lib/pkgconfig"

PATH="$HOME/.local/bin:\
~/perl5/bin:\
~/.meteor:\
~/.cargo/bin:\
~/.pyenv/shims:\
~/.pyenv/bin:\
/usr/local/bin:\
/usr/bin:\
/usr/local/sbin:\
/usr/sbin:\
/usr/local/opt/llvm@5/bin:\
/usr/local/opt/llvm/bin:\
/usr/local/opt/libxml2/bin:\
/opt/subversion/bin:\
/usr/local/opt/coreutils/libexec/gnubin:\
$ADOBE_AIR_SDK/bin:\
$ANT_HOME/bin:\
$BREW_HOME/bin:\
$BREW_HOME/share/python:\
$CATALINA_HOME/bin:\
$CF_HOME/bin:\
$COREUTILS:\
$GOROOT/bin:\
$GRADLE_HOME/bin:\
$GRAILS_HOME/bin:\
$GROOVY_HOME/bin:\
$HEROKU_HOME/bin:\
$JETTY_HOME/bin:\
$JRUBY_HOME/bin:\
$M2_HOME\bin:\
$MONGODB_HOME/bin:\
$MYSQL_PATH/bin:\
$MY_HOME/bin:\
$NGINX_HOME/sbin:\
$RUBY_HOME/bin:\
$WEBMIN_HOME:\
$PATH"

export ACTIVEMQ_HOME
export ANDROID_HOME
export ANT_HOME
export BREW_HOME
export CATALINA_HOME
export CF_HOME
export CF_ROOT
export CLASSPATH
export CPATH
export DROPBOX_HOME
export EC2_HOME
export FLEX_SDK
export GOROOT
export GRADLE_HOME
export GRAILS_HOME
export GROOVY_HOME
export HEROKU_HOME
export JAVA_HOME
export JAVA_OPTS
export JDK_HOME
export JVM_HOME
export JENKINS_HOME
export JETTY_HOME
export JETTY_BASE
export JRUBY_HOME
export JYTHON_HOME
export JYTHONPATH
export LIQUIBASE_HOME
export M2_HOME
export MAVEN_OPTS
export MONGODB_HOME
export MYSQL_PATH
export NGINX_HOME
export PYTHON_HOME
export PYTHONPATH
export RUBY_HOME
export WEBMIN_HOME
export PERL5LIB
export PERL_LOCAL_LIB_ROOT
export PERL_MB_OPT
export PERL_MM_OPT
export BOGUS
export CC="/usr/bin/gcc"
export LDFLAGS
export CPPFLAG
export PKG_CONFIG_PATH
export PATH
